{
  "name": "@mapineda48/personal",
  "version": "0.0.12",
  "description": "Personal web page in expressjs route.",
  "keywords": [
    "expressjs",
    "page",
    "react",
    "react-router",
    "single-page",
    "personal"
  ],
  "author": "Miguel Pineda <a.pinedavegamiguel@gmail.com>",
  "license": "MIT",
  "main": "lib",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mapineda48/njs.git",
    "directory": "personal"
  },
  "scripts": {
    "frontend": "mp",
    "frontend:types": "tsc -P frontend/tsconfig.json --noEmit",
    "frontend:build": "rm -rf frontend/build && mp -B -A -U /",
    "frontend:model": "ts-node frontend/model.ts",
    "frontend:render": "ts-node frontend/render.ts",
    "frontend:dist": "yarn frontend:build && yarn frontend:model && yarn frontend:render",
    "types": "tsc --noEmit",
    "build:lib": "tsc",
    "check:types": "tsc && tsc -P lib/tsconfig.json --noEmit",
    "build": "rm -rf dist && yarn build:lib && yarn frontend:dist",
    "postbuild": "ts-node postbuild.ts",
    "version": "yarn build",
    "release": "yarn mp48-release"
  },
  "bugs": {
    "url": "https://github.com/mapineda48/njs/issues"
  },
  "dependencies": {
    "@mapineda48/demos-api-rest": "0.0.3",
    "@mapineda48/demos-grants": "^0.0.2",
    "@mapineda48/demos-sigma": "^0.0.8",
    "@mapineda48/social": "0.0.14",
    "@popperjs/core": "^2.9.1",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/aos": "^3.0.4",
    "@types/express": "^4.17.11",
    "@types/fs-extra": "^9.0.8",
    "@types/jest": "^26.0.15",
    "@types/morgan": "^1.9.2",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-helmet": "^6.1.0",
    "@types/react-router-dom": "^5.1.7",
    "aos": "^2.3.4",
    "axios": "^0.21.1",
    "bootstrap": "^5.1.3",
    "clsx": "^1.1.1",
    "dotenv": "8.2.0",
    "express": "^4.17.1",
    "fs-extra": "^9.1.0",
    "helmet": "^4.6.0",
    "morgan": "^1.10.0",
    "mp48-cra": "0.0.3",
    "mp48-react": "0.0.2",
    "mp48-util": "^0.0.3",
    "nodemon": "^2.0.7",
    "normalize.css": "^8.0.1",
    "puppeteer": "^8.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-helmet-async": "^1.1.2",
    "react-icons": "^4.3.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "sass": "1.34.0",
    "smooth-scroll-into-view-if-needed": "^1.1.30",
    "socket.io": "^4.2.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.2",
    "web-vitals": "^1.0.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "nodemonConfig": {
    "execMap": {
      "ts": "ts-node -r dotenv/config"
    }
  },
  "private": true,
  "devDependencies": {
    "@types/pg": "^8.6.1",
    "pg": "^8.7.1"
  }
}
