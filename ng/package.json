{
  "name": "mp48-ng",
  "version": "0.0.1",
  "private": true,
  "author": "Miguel Angel Pineda Vega",
  "license": "MIT",
  "description": "Minimal overwrite ng cli",
  "bugs": {
    "url": "https://github.com/mapineda48/njs/issues"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/mapineda48/njs.git",
    "directory": "ng"
  },
  "engines": {
    "node": ">=14",
    "npm": ">=7",
    "yarn": ">=1.22.5"
  },
  "scripts": {
    "ng": "ng",
    "release": "yarn mp48-release",
    "version": "yarn build",
    "build": "tsc && rm -rf dist/app dist/angular.json",
    "postbuild": "ts-node postbuild.ts",
    "app:start": "ng serve",
    "app:build": "ng build",
    "app:watch": "ng build --watch --configuration development",
    "app:test": "ng test"
  },
  "dependencies": {
    "@angular/animations": "~12.2.0",
    "@angular/common": "~12.2.0",
    "@angular/compiler": "~12.2.0",
